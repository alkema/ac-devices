<p id="notice"><%= notice %></p>

<h1>Api V1 Devices</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_devices.each do |api_v1_device| %>
      <tr>
        <td><%= link_to 'Show', api_v1_device %></td>
        <td><%= link_to 'Edit', edit_api_v1_device_path(api_v1_device) %></td>
        <td><%= link_to 'Destroy', api_v1_device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Device', new_api_v1_device_path %>
